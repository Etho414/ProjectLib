          CanAttackDisablerBut.Text = "Always Attack: "..tostring(_G.CanAttackDisabler)
     end)






end
CreateSetting()
function LagServer()
     game:GetService("RunService").RenderStepped:Connect(function()
          if _G.LagServer == true then 
              
               fireproximityprompt(workspace.Capture.CapturePart.ProximityPrompt)
          end 
     end)
end 
for i = 1, 50 do
     
coroutine.wrap(LagServer)()


end
