     FlyspeedBox.Font = Enum.Font.SourceSans
     FlyspeedBox.Text = "10"
     FlyspeedBox.TextColor3 = Color3.fromRGB(0, 0, 0)
     FlyspeedBox.TextScaled = true
     FlyspeedBox.TextSize = 14.000
     FlyspeedBox.TextWrapped = true
     FlyspeedBox.Changed:connect(function()
          if FlyspeedBox.Text == "" then return end 
          if tonumber(FlyspeedBox.Text) == nil then return end
          _G.FlySpeed = tonumber(FlyspeedBox.Text)
          game.Players.LocalPlayer.Character.Config.SpeedBoost.Value = _G.FlySpeed
     
     end)
     CanAttackDisablerBut.Name = "LagServerBut"
     CanAttackDisablerBut.Parent = Main
     CanAttackDisablerBut.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
