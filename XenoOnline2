     SetBindEsp.TextColor3 = Color3.fromRGB(0, 0, 0)
     SetBindEsp.TextScaled = true
     SetBindEsp.TextSize = 14.000
     SetBindEsp.TextWrapped = true

     local ChangingESPBind = false

     SetBindEsp.MouseButton1Click:connect(function()
          ChangingESPBind = true
           SetBindEsp.Text = "Changing Bind..."
     end)
     game:GetService("UserInputService").InputBegan:connect(function(i)
          if ChangingESPBind == true then
               if i.KeyCode ~= Enum.KeyCode.Unknown then
                    _G.EspKey = i.KeyCode.Name
                    SetBindEsp.Text = "Change ESP Bind: ".._G.EspKey  
