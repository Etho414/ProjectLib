     ShowHealthBut.Size = UDim2.new(0, 149, 0, 29)
     ShowHealthBut.Font = Enum.Font.SourceSans
     ShowHealthBut.Text = "Show Health: "..tostring(_G.ShowHp)
     ShowHealthBut.TextColor3 = Color3.fromRGB(0, 0, 0)
     ShowHealthBut.TextScaled = true
     ShowHealthBut.TextSize = 14.000
     ShowHealthBut.TextWrapped = true

     ShowHealthBut.MouseButton1Click:connect(function()
          _G.ShowHp = not _G.ShowHp
          ShowHealthBut.Text = "Show Health: "..tostring(_G.ShowHp)
     end)
    
     UsePercentageHealthBut.Name = "UsePercentageHealthBut"
     UsePercentageHealthBut.Parent = Main
     UsePercentageHealthBut.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
     UsePercentageHealthBut.Position = UDim2.new(0.125, 0, 0.320143878, 0)
     UsePercentageHealthBut.Size = UDim2.new(0, 149, 0, 29)
     UsePercentageHealthBut.Font = Enum.Font.SourceSans
     UsePercentageHealthBut.Text = "Use Percentage: "..tostring(_G.UsePercentage)
     UsePercentageHealthBut.TextColor3 = Color3.fromRGB(0, 0, 0)
     UsePercentageHealthBut.TextScaled = true
     UsePercentageHealthBut.TextSize = 14.000
     UsePercentageHealthBut.TextWrapped = true
     UsePercentageHealthBut.MouseButton1Click:connect(function()
          _G.UsePercentage = not _G.UsePercentage
          UsePercentageHealthBut.Text = "Use Percentage: "..tostring(_G.UsePercentage)
     end)


     ShowLifeForceBut.Name = "ShowLifeForceBut"
     ShowLifeForceBut.Parent = Main
     ShowLifeForceBut.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
     ShowLifeForceBut.Position = UDim2.new(0.125, 0, 0.446043164, 0)
     ShowLifeForceBut.Size = UDim2.new(0, 149, 0, 29)
     ShowLifeForceBut.Font = Enum.Font.SourceSans
     ShowLifeForceBut.Text = "Show Life Force: "..tostring(_G.ShowLifeForce)
     ShowLifeForceBut.TextColor3 = Color3.fromRGB(0, 0, 0)
     ShowLifeForceBut.TextScaled = true
     ShowLifeForceBut.TextSize = 14.000
     ShowLifeForceBut.TextWrapped = true
     ShowLifeForceBut.MouseButton1Click:connect(function()
          _G.ShowLifeForce = not _G.ShowLifeForce
          ShowLifeForceBut.Text = "Show Life Force: "..tostring(_G.ShowLifeForce)
     end)



     LagServerBut.Name = "LagServerBut"
     LagServerBut.Parent = Main
